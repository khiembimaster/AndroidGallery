<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_photos">

    <fragment
        android:id="@+id/navigation_library"
        android:name="android21ktpm3.group07.androidgallery.ui.library.LibraryFragment"
        android:label="@string/title_library"
        tools:layout="@layout/fragment_library" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="android21ktpm3.group07.androidgallery.ui.search.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_photos"
        android:name="android21ktpm3.group07.androidgallery.ui.photos.PhotosFragment"
        android:label="@string/title_photos"
        tools:layout="@layout/fragment_photos" />
    <fragment
        android:id="@+id/navigation_memories"
        android:name="android21ktpm3.group07.androidgallery.ui.memories.MemoriesFragment"
        android:label="@string/title_memories"
        tools:layout="@layout/fragment_memories" />

    <fragment
        android:id="@+id/navigation_slideShow"
        android:name="android21ktpm3.group07.androidgallery.ui.SlideShow.SlideShowFragment"
        android:label="@string/title_slideShow"
        tools:layout="@layout/fragment_slideshow" />
</navigation>